{
    "name": "brouzie/specificator",
    "description": "Framework for querying data from read model using Specification pattern. Allows building dynamic queries from reusable components (filters, sort orders, aggregations",
    "type": "library",
    "keywords": [
        "specification",
        "aggregations"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Myakshin",
            "email": "molodchick@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^7.0",
        "psr/container": "^1.0",
        "ruflin/elastica": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Brouzie\\Specificator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brouzie\\Specificator\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
